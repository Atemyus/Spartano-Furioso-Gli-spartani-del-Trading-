{
  "name": "spartano-furioso-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:js": "node index.js",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "start:js": "node index.js",
    "admin:manage": "node scripts/makeAdmin.js",
    "admin:update": "node scripts/updateAdminCredentials.cjs",
    "admin:create": "node scripts/createAdmin.js",
    "admin:check": "node scripts/checkAdmin.js",
    "sync:names": "node scripts/syncProductNames.js",
    "trials:verify": "node scripts/verifyTrialExpiration.js",
    "db:sync": "node scripts/verifySyncDatabase.js",
    "db:seed": "node scripts/seedDatabase.js",
    "db:migrate": "node scripts/migrateToMongoDB.js",
    "db:migrate:prisma": "node scripts/migratePrismaToMongoDB.js",
    "db:setup": "node scripts/autoSetup.js",
    "orders:verify": "node scripts/verifyOrdersSubscriptions.js",
    "fury:update-platforms": "node scripts/updateFuryPlatforms.cjs",
    "products:check": "node scripts/checkProducts.js",
    "products:export": "node scripts/exportProducts.js",
    "products:import": "node scripts/importProducts.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:push": "prisma db push",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "stripe": "^18.5.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.6.0",
    "@types/nodemailer": "^7.0.1",
    "mongodb-memory-server": "^10.2.0",
    "prisma": "^5.18.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.0"
  }
}
